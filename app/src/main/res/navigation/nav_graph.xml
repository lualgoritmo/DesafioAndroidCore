<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentHome">

    <fragment
        android:id="@+id/fragmentHome"
        android:name="br.com.zup.desafioandroidcore.Fragments.fragmentHome"
        android:label="fragmentHome"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_fragmentHome_to_fragmentoCadastrarProduto"
            app:destination="@id/fragmentoCadastrarProduto" />
    </fragment>
    <fragment
        android:id="@+id/fragmentoCadastrarProduto"
        android:name="br.com.zup.desafioandroidcore.Fragments.FragmentoCadastrarProduto"
        android:label="fragment_cadastrar_produto"
        tools:layout="@layout/fragment_cadastrar_produto">
        <action
            android:id="@+id/action_fragmentoCadastrarProduto_to_produtosCadastrados2"
            app:destination="@id/produtosCadastrados" />
    </fragment>

    <fragment
        android:id="@+id/produtosCadastrados"
        android:name="br.com.zup.desafioandroidcore.Fragments.ProdutosCadastrados"
        android:label="fragment_produtos_cadastrados"
        tools:layout="@layout/fragment_produtos_cadastrados" />
</navigation>